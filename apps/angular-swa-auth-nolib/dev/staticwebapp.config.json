{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/.auth/login/aad",
      "statusCode": 401
    },
    {
      "route": "/.auth/login/google",
      "statusCode": 401
    },
    {
      "route": "/.auth/login/facebook",
      "statusCode": 401
    },
    {
      "route": "/.auth/login/twitter",
      "statusCode": 401
    },
    {
      "route": "/login",
      "rewrite": "/.auth/login/github"
    },
    {
      "route": "/logged-out",
      "rewrite": "/assets/logged-out.html"
    },
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/login",
      "statusCode": 302
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["*.{css,scss,js,png,gif,ico,jpg,svg}"]
  },
  "globalHeaders": {
    "etag": "",
    "Cache-Control": "",
    "cache-control": ""
  }
}
