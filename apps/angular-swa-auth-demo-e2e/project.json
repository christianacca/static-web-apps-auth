{
  "root": "apps/angular-swa-auth-demo-e2e",
  "sourceRoot": "apps/angular-swa-auth-demo-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "start-server-and-test 'nx serve-swa-with-api' 4280 'nx _e2e angular-swa-auth-demo-e2e {args.opts}'"
        ]
      },
      "configurations": {
        "ci": {
          "commands": [
            "start-server-and-test 'nx serve-swa-with-api' 4280 'nx _e2e angular-swa-auth-demo-e2e --configuration=ci'"
          ]
        }
      }
    },
    "_e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/angular-swa-auth-demo-e2e/cypress.json",
        "tsConfig": "apps/angular-swa-auth-demo-e2e/tsconfig.e2e.json",
        "devServerTarget": "angular-swa-auth-demo:echo-dev-server-url",
        "baseUrl": "http://localhost:4280"
      },
      "configurations": {
        "ci": {
          "headless": true,
          "record": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/angular-swa-auth-demo-e2e/**/*.{js,ts}"]
      },
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": ["scope:angular-swa-auth-demo", "type:e2e", "framework:angular"],
  "implicitDependencies": ["angular-swa-auth-demo"]
}
