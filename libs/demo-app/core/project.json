{
  "projectType": "library",
  "root": "libs/demo-app/core",
  "sourceRoot": "libs/demo-app/core/src",
  "prefix": "app",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/demo-app/core"],
      "options": {
        "jestConfig": "libs/demo-app/core/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/demo-app/core/src/**/*.ts", "libs/demo-app/core/src/**/*.html"]
      }
    }
  },
  "tags": ["framework:angular", "type:util"]
}
